<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
	version="2.1">
	
	<managed-bean>
		<managed-bean-name>beanAmis</managed-bean-name>
		<managed-bean-class>BeanAmis</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	
	<managed-bean>
		<managed-bean-name>beanInscription</managed-bean-name>
		<managed-bean-class>BeanInscription</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	
	<managed-bean>
		<managed-bean-name>beanMedia</managed-bean-name>
		<managed-bean-class>BeanMedia</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>beanMessagesPrives</managed-bean-name>
		<managed-bean-class>BeanMessagesPrives</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	
	<managed-bean>
		<managed-bean-name>beanConnexion</managed-bean-name>
		<managed-bean-class>BeanConnexion</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>password</property-name>
			<property-class>java.lang.String</property-class>
			<value />
		</managed-property>
		<managed-property>
			<property-name>identifiant</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>connected</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>beanProfil</managed-bean-name>
		<managed-bean-class>BeanProfil</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>beanRecuperationMDP</managed-bean-name>
		<managed-bean-class>BeanRecuperationMDP</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>beanGestionPlaylist</managed-bean-name>
		<managed-bean-class>BeanGestionPlaylist</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>beanGestionUtilisateursAdmin</managed-bean-name>
		<managed-bean-class>BeanGestionUtilisateursAdmin</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>beanGestionMediasAdmin</managed-bean-name>
		<managed-bean-class>BeanGestionMediasAdmin</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>beanGestionSignalementsAdmin</managed-bean-name>
		<managed-bean-class>BeanGestionSignalementsAdmin</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>beanUpload</managed-bean-name>
		<managed-bean-class>BeanUpload</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>beanHome</managed-bean-name>
		<managed-bean-class>BeanHome</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>beanNotification</managed-bean-name>
		<managed-bean-class>BeanNotification</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>isConnected</from-outcome>
			<to-view-id>/pages/home.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>isNotConnected</from-outcome>
			<to-view-id>/pages/home.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
</faces-config>
