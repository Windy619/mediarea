<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <managed-bean>
  <managed-bean-name>beanAmis</managed-bean-name>
  <managed-bean-class>BeanAmis</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>beanInscription</managed-bean-name>
  <managed-bean-class>BeanInscription</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>adrMail</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>confirm</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>password</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>beanMedia</managed-bean-name>
  <managed-bean-class>BeanMedia</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>mediaVisualise</property-name>
   <property-class>metier.media.Media</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>beanMessagesPrives</managed-bean-name>
  <managed-bean-class>BeanMessagesPrives</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>beanConnexion</managed-bean-name>
  <managed-bean-class>BeanConnexion</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>password</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>identifiant</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>connected</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>beanSuiteInscription</managed-bean-name>
  <managed-bean-class>BeanSuiteInscription</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>pseudo</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>nomUtilisateur</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>prenomUtilisateur</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>beanProfil</managed-bean-name>
  <managed-bean-class>BeanProfil</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>beanRecherche</managed-bean-name>
  <managed-bean-class>BeanRecherche</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
  	<property-name>afficher_par</property-name>
  	<property-class>java.lang.String</property-class>
  	<value>1</value>
  </managed-property>
  <managed-property>
  	<property-name>tri</property-name>
  	<property-class>java.lang.String</property-class>
  	<value>1</value>
  </managed-property>
  <managed-property>
  	<property-name>tri_ordre</property-name>
  	<property-class>java.lang.String</property-class>
  	<value>2</value>
  </managed-property>
  <managed-property>
  	<property-name>nb_par_pages</property-name>
  	<property-class>java.lang.String</property-class>
  	<value>15</value>
  </managed-property>
  <managed-property>
  	<property-name>seulementTelechargeable</property-name>
  	<property-class>java.lang.Boolean</property-class>
  	<value>false</value>
  </managed-property>
  <managed-property>
  	<property-name>typeMedia</property-name>
  	<property-class>java.lang.String</property-class>
  	<value>1</value>
  </managed-property>
  <managed-property>
  	<property-name>typeRecherche</property-name>
  	<property-class>java.lang.String</property-class>
  	<value>1</value>
  </managed-property>
  <managed-property>
  	<property-name>page</property-name>
  	<property-class>java.lang.String</property-class>
  	<value>1</value>
  </managed-property>
 </managed-bean>
 <navigation-rule>
   <from-view-id>*</from-view-id> 
   <navigation-case> 
     <from-outcome>rechercheMedias</from-outcome> 
     <to-view-id>/pages/recherche.xhtml</to-view-id> 
   </navigation-case>
  </navigation-rule> 
  <navigation-rule>
  <from-view-id>/pages/monCompte.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>isConnected</from-outcome>
   <to-view-id>/pages/home.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>isNotConnected</from-outcome>
   <to-view-id>/pages/monCompte.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/monCompte.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>isCreated</from-outcome>
   <to-view-id>/pages/suiteInscription.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>isNotCreated</from-outcome>
   <to-view-id>/pages/monCompte.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/suiteInscription.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>isUpdated</from-outcome>
   <to-view-id>/pages/profil.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
</faces-config>
